{
  "name": "@4513echo/ddu-kind-url",
  "version": "0.0.0",
  "exports": "./denops/@ddu-kinds/url.ts",
  "publish": {
    "include": [
      "**/*.ts"
    ]
  },
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "useUnknownInCatchVariables": true
  },
  "lint": {
    "rules": {
      "include": [
        "eqeqeq"
      ],
      "tags": [
        "jsr",
        "recommended"
      ]
    }
  },
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts"
  }
}
